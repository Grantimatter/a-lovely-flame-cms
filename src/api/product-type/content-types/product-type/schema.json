{
  "kind": "collectionType",
  "collectionName": "product_types",
  "info": {
    "singularName": "product-type",
    "pluralName": "product-types",
    "displayName": "ProductType",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "Title": {
      "type": "string"
    },
    "Plural": {
      "type": "string"
    },
    "Slug": {
      "type": "uid",
      "required": true
    },
    "SortOrder": {
      "type": "integer"
    },
    "WhyLove": {
      "type": "richtext"
    },
    "Usage": {
      "type": "richtext"
    },
    "Ingredients": {
      "type": "richtext"
    },
    "Products": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::product.product",
      "mappedBy": "ProductType"
    },
    "Addons": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::product.product",
      "mappedBy": "ParentProduct"
    },
    "Fragrances": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::fragrance.fragrance",
      "inversedBy": "ProductTypes"
    },
    "Price": {
      "type": "decimal"
    }
  }
}
